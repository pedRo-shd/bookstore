<h1>Autenticação</h1>
  <p><%= flash[:notice] %></p>
    <%= form_tag sessions_path do %>
  <p>
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </p>
  <p>
    <%= label_tag :senha %>
    <%= password_field_tag :senha %>
  </p>
  <p>
    <%= submit_tag 'Autenticar' %>
  </p>
<% end %>
